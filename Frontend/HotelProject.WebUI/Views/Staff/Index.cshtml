@model List<StaffViewModel>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayoutController1/_AdminLayout.cshtml";
}

<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Personel Adı</th>
        <th>Personel Ünvanı</th>
        <th>Sil</th>
        <th>Güncelle</th>
        <th>Detaylar</th>
        


    </tr>
    @foreach(var item in Model)
    {
        <tr>
            <td>@item.StaffID</td>
            <td>@item.Name</td>
            <td>@item.Title</td>
            <td><a href="#" class="btn btn-danger">Sil</a></td>
            <td><a href="#" class="btn btn-success">Güncelle</a></td>
            <td><a href="#" class="btn btn-dark">Detaylar</a></td>
            
        </tr>
    }
</table>
<a href="#" class="btn btn-info">Yeni Personel</a>

